.insert-form{
  width: 100%;
  box-sizing: border-box;
  padding: 10rpx 20rpx;
}
.insert-form-title{
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 14px;
  font-weight: 600; 
  box-sizing: border-box;
  /* border-left: 4px solid #000;  */
  padding-left:10rpx;
  /* border-bottom: 1px solid #eee; */
}
.insert-form-item{
  display: flex;
  width: 100%;
  border-bottom: 1px solid #eee;
  padding: 10rpx 0;
}
.insert-form-item-name{
  flex-grow: 3;
  text-align: right;
  height: 80rpx;
  line-height: 80rpx;
  box-sizing: border-box;
  padding-right:10rpx; 
  width: 0px;
}
.insert-form-item-input{
  flex-grow: 8;
  width: 0px;
}
.insert-form-item-input input{
  border: none;
  margin-top: 18rpx;
}
.insert-form-item-must{
  font-size:24rpx;
  font-family:PingFangSC-Medium;
  font-weight:500;
  color:rgba(254,0,0,1);
  margin-right:8rpx;
}
.sellorcollect{
  display: inline-block;
  margin-right: 40rpx;
  color:#333;
}
.sellorcollect image{
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
}
.sellorcollect image{
  display: inline-block;
  margin-right: 20rpx;
  position: relative;
  top: 10rpx;
}
.form-item-placeholder{
  margin-top:16rpx;
}
